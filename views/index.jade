extends layout

block content
	h1= title
	p Welcome to #{title}
		video(id='video', class='video-js vjs-default-skin', controls, preload='none', width='640', height='264', poster='http://video-js.zencoder.com/oceans-clip.png' )
			source(src='#{url_video}', type='video/mp4')
			source(src="#{url_video}",type='video/webm')
			source(src="#{url_video}",type='video/ogg')

	input(type='button',value='Principio',onclick='mover2()',class='botones')
	input(type='button',value='Final',onclick='mover1()',class='botones')


